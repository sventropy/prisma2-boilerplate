datasource db {
  provider = "sqlite"
  url      = "sqlite:./dev.db"
}

generator prisma_client {
  provider = "prisma-client-js"
}

model Header {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  title     String
  items     Item[]
}

model Item {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  title     String?
  type      ItemType
}

enum ItemType {
  A
  B
  C
}
